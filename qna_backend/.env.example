# Copy this file to .env and fill in the values.
# The backend will load this file automatically when running via manage.py, ASGI, or WSGI.

# Gemini API key (required for real AI responses)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: choose model variant (default: gemini-1.5-flash)
GEMINI_MODEL=gemini-1.5-flash

# Set to "true" to force mocked AI responses (useful for local dev without a key)
USE_MOCK_AI=false

# Optional: LangChain tracing settings
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=
